{
  "name": "@rozoai/pos",
  "version": "0.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "DARK_MODE=media cross-env  expo run:android",
    "android:production": "cross-env APP_ENV=production pnpm run android",
    "android:staging": "cross-env APP_ENV=staging pnpm run android",
    "app-release": "cross-env SKIP_BRANCH_PROTECTION=true np --no-publish --no-cleanup --no-release-draft",
    "build:development:android": "cross-env APP_ENV=development  eas build --profile development --platform android ",
    "build:development:ios": "cross-env APP_ENV=development  eas build --profile development --platform ios",
    "build:production:android": "cross-env APP_ENV=production  eas build --profile production --platform android ",
    "build:production:ios": "cross-env APP_ENV=production  eas build --profile production --platform ios",
    "build:staging:android": "cross-env APP_ENV=staging  eas build --profile staging --platform android ",
    "build:staging:ios": "cross-env APP_ENV=staging  eas build --profile staging --platform ios",
    "check-all": "pnpm run lint && pnpm run type-check && pnpm run lint:translations",
    "doctor": "npx expo-doctor@latest",
    "expo:add": "npx expo install",
    "format": "prettier \"src/**/*.ts{,x}\" --write",
    "ios": "DARK_MODE=media cross-env  expo run:ios",
    "ios:production": "cross-env APP_ENV=production pnpm run ios",
    "ios:staging": "cross-env APP_ENV=staging pnpm run ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:translations": "eslint ./src/translations/ --fix --ext .json  ",
    "prebuild": "cross-env  pnpm expo prebuild",
    "prebuild:development": "cross-env APP_ENV=development pnpm run prebuild",
    "prebuild:production": "cross-env APP_ENV=production pnpm run prebuild",
    "prebuild:staging": "cross-env APP_ENV=staging pnpm run prebuild",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "start": "cross-env  expo start --clear",
    "start:production": "cross-env APP_ENV=production pnpm run start",
    "start:staging": "cross-env APP_ENV=staging pnpm run start",
    "type-check": "tsc  --noemit",
    "version": "pnpm run prebuild && git add .",
    "web": "DARK_MODE=media cross-env  expo start --web --clear",
    "web:staging": "DARK_MODE=media cross-env APP_ENV=staging expo start --web",
    "web:production": "DARK_MODE=media cross-env APP_ENV=production expo start --web",
    "xcode": "xed -b ios"
  },
